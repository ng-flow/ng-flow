{
  "name": "@ng-bucket/forms",
  "description": "Missing pieces for Angular Developer - Forms",
  "author": "Micha≈Ç Kurcius <mkurcius@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:ng-bucket/ng-bucket.git",
  "keywords": [
    "angular",
    "reactive forms",
    "forms",
    "AbstractControl",
    "FormControl",
    "FormGroup",
    "FormArray",
    "pristine",
    "dirty",
    "touched",
    "untouched",
    "enable",
    "enableAll",
    "dirtyValue",
    "dirtyValues"
  ],
  "version": "1.2.0",
  "peerDependencies": {
    "@angular/forms": ">= 6.x.x <= 8.x.x",
    "rxjs": ">=6.x.x"
  },
  "scripts": {
    "lint": "ng lint forms",
    "test": "ng test forms",
    "build": "ng build forms",
    "copy:dist": "cp-cli ../../dist/forms dist",
